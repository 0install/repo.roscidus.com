<interface uri="http://repo.roscidus.com/kubernetes/kompose" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <replaced-by interface="https://apps.0install.net/kubernetes/kompose.xml"/>
  <feed src="https://apps.0install.net/kubernetes/kompose.xml"/>
  <name>Kompose</name>
  <summary>go from Docker Compose to Kubernetes</summary>
  <description>Kompose is a tool to help users who are familiar with docker-compose move to Kubernetes. kompose takes a Docker Compose file and translates it into Kubernetes resources.</description>
  <homepage>http://kompose.io/</homepage>
  <needs-terminal/>

  <entry-point binary-name="kompose" command="run"/>
</interface>
