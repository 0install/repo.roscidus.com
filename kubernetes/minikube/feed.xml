<interface uri="http://repo.roscidus.com/kubernetes/minikube" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <replaced-by interface="https://apps.0install.net/kubernetes/minikube.xml"/>
  <feed src="https://apps.0install.net/kubernetes/minikube.xml"/>
  <name>Minikube</name>
  <summary>run Kubernetes locally</summary>
  <description>Minikube is a tool that makes it easy to run Kubernetes locally. Minikube runs a single-node Kubernetes cluster inside a VM on your laptop for users looking to try out Kubernetes or develop with it day-to-day.</description>
  <homepage>https://github.com/kubernetes/minikube</homepage>
  <needs-terminal/>

  <entry-point binary-name="minikube" command="run"/>
</interface>
