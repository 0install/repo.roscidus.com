<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='../../resources/feed.xsl'?>
<interface uri="http://repo.roscidus.com/cli/mono" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>Mono</name>
  <summary>open-source cross-platform implementation of the Common Language Infrastructure (.NET Framework compatible)</summary>
  <homepage>http://www.mono-project.com/</homepage>
  <feed-for interface="http://repo.roscidus.com/cli/cli"/>
  <feed-for interface="http://repo.roscidus.com/cli/cli-monopath"/>

  <group main="/usr/bin/mono">
    <!-- There is a separate GUI version of the Mono executable on Windows. On POSIX systems we simply map it back to regular Mono. -->
    <command name="run-gui">
      <runner interface="http://repo.roscidus.com/cli/mono"/>
    </command>
    <package-implementation distributions="Debian" package="mono-complete"/>
    <package-implementation distributions="RPM" package="mono-winforms"/>
    <package-implementation distributions="Arch" package="mono"/>
  </group>

  <group arch="Windows-*" license="MIT License">
    <command name="run" path="bin/mono.exe"/>
    <command name="run-gui" path="bin/monow.exe"/>
    <implementation id="sha1new=5786d36753bb1f76e4d4f25b2980749ce3671aab" version="2.10.9">
      <manifest-digest sha1new="5786d36753bb1f76e4d4f25b2980749ce3671aab" sha256="c5766eb07b242dc548e4af84671f07ad5e114f73b07317a0e96237ee553f761d" sha256new="YV3G5MD3EQW4KSHEV6CGOHYHVVPBCT3TWBZRPIHJMI364VJ7OYOQ"/>
      <archive href="http://repo.roscidus.com/archives/Mono/mono-2.10.9-win32.tar.lzma" size="83591462" type="application/x-lzma-compressed-tar"/>
    </implementation>
  </group>
</interface>
