<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='feed.xsl'?>
<interface uri="http://repo.roscidus.com/utils/xz" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <replaced-by interface="https://apps.0install.net/utils/xz.xml"/>
  <feed src="https://apps.0install.net/utils/xz.xml"/>
  <name>XZ Utils</name>
  <summary>xz data compression tool for .xz and .lzma files</summary>
  <description>XZ Utils is free general-purpose data compression software with high compression ratio. XZ Utils are the successor to LZMA Utils.</description>
  <homepage>http://tukaani.org/xz/</homepage>
  <category>Utility</category>
  <needs-terminal/>

  <entry-point command="run" binary-name="xz">
    <name>compress .xz file</name>
    <needs-terminal/>
  </entry-point>
  <entry-point command="unxz">
    <name>decompress .xz file</name>
    <needs-terminal/>
  </entry-point>
  <entry-point command="xzcat">
    <name>decompress .xz file to stdout</name>
    <needs-terminal/>
  </entry-point>
  <entry-point command="lzma">
    <name>compress .lzma file</name>
    <needs-terminal/>
  </entry-point>
  <entry-point command="unlzma">
    <name>decompress .lzma file</name>
    <needs-terminal/>
  </entry-point>
  <entry-point command="lzcat">
    <name>decompress .lzma file to stdout</name>
    <needs-terminal/>
  </entry-point>
</interface>
