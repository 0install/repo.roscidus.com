<?xml version="1.0" ?>
<interface uri="http://repo.roscidus.com/utils/putty" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <replaced-by interface="https://apps.0install.net/gui/putty.xml"/>
  <feed src="https://apps.0install.net/gui/putty.xml"/>
  <name>PuTTY</name>
  <summary xml:lang="en">Telnet and SSH client</summary>
  <summary xml:lang="de">Telnet und SSH Klient</summary>
  <description xml:lang="en">PuTTY is a free implementation of Telnet and SSH for Windows and Unix platforms, along with an xterm terminal emulator.</description>
  <description xml:lang="de">PuTTY ist eine freie Implementierung von Telnet und SSH f√ºr Windows und Unix Plattformen kombiniert mit einem xterm Terminal Emulator.</description>
  <homepage>https://www.chiark.greenend.org.uk/~sgtatham/putty/</homepage>
  <icon href="https://raw.githubusercontent.com/0install/apps.0install.net/master/gui/putty.png" type="image/png"/>
  <icon href="https://raw.githubusercontent.com/0install/apps.0install.net/master/gui/putty.ico" type="image/vnd.microsoft.icon"/>
  <category>Network</category>
  <category>Utility</category>
  <entry-point binary-name="putty" command="run"/>
  <entry-point command="puttygen">
    <name xml:lang="en">PuTTY Key Generator</name>
    <icon href="https://raw.githubusercontent.com/0install/apps.0install.net/master/gui/putty/puttygen.png" type="image/png"/>
    <icon href="https://raw.githubusercontent.com/0install/apps.0install.net/master/gui/putty/puttygen.ico" type="image/vnd.microsoft.icon"/>
  </entry-point>
  <entry-point command="pageant">
    <name xml:lang="en">PuTTY authentication agent</name>
    <icon href="https://raw.githubusercontent.com/0install/apps.0install.net/master/gui/putty/pageant.png" type="image/png"/>
    <icon href="https://raw.githubusercontent.com/0install/apps.0install.net/master/gui/putty/pageant.ico" type="image/vnd.microsoft.icon"/>
  </entry-point>
  <entry-point command="plink">
    <needs-terminal/>
    <name xml:lang="en">PuTTY Plink</name>
    <icon href="https://raw.githubusercontent.com/0install/apps.0install.net/master/gui/putty/plink.png" type="image/png"/>
    <icon href="https://raw.githubusercontent.com/0install/apps.0install.net/master/gui/putty/plink.ico" type="image/vnd.microsoft.icon"/>
  </entry-point>
  <entry-point command="pscp">
    <needs-terminal/>
    <name xml:lang="en">PuTTY Secure Copy</name>
    <icon href="https://raw.githubusercontent.com/0install/apps.0install.net/master/gui/putty/pscp.png" type="image/png"/>
    <icon href="https://raw.githubusercontent.com/0install/apps.0install.net/master/gui/putty/pscp.ico" type="image/vnd.microsoft.icon"/>
  </entry-point>
  <entry-point command="psftp">
    <needs-terminal/>
    <name xml:lang="en">PuTTY SFTP</name>
    <icon href="https://raw.githubusercontent.com/0install/apps.0install.net/master/gui/putty/psftp.png" type="image/png"/>
    <icon href="https://raw.githubusercontent.com/0install/apps.0install.net/master/gui/putty/psftp.ico" type="image/vnd.microsoft.icon"/>
  </entry-point>
</interface>
