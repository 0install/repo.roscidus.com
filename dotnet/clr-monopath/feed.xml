<interface uri="http://repo.roscidus.com/dotnet/clr-monopath" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>Common Language Infrastructure with MONO_PATH support</name>
  <summary>an implementation of the Common Language Infrastucture (.NET Framework on Windows, Mono on POSIX) using .NET profiles as version numbers</summary>

  <!-- Empty implementations used to map .NET Framework version numbers to roughly equivalent Mono version numbers. -->
  <feed arch="POSIX-*" src="http://repo.roscidus.com/dotnet/clr"/>

  <!-- The monopath-emulator adds support for MONO_PATH to the regular .NET Framework  -->
  <group arch="Windows-*" license="MIT License">
    <implementation id="netfx-2.0" version="2.0">
      <command name="run" path="monopath-emulator.clr2.exe">
        <runner command="run" interface="http://repo.roscidus.com/dotnet/framework">
          <version before="3.0" not-before="2.0"/>
        </runner>
      </command>
      <command name="run-gui" path="monopath-emulator-win.clr2.exe">
        <runner command="run-gui" interface="http://repo.roscidus.com/dotnet/framework">
          <version before="3.0" not-before="2.0"/>
        </runner>
      </command>
